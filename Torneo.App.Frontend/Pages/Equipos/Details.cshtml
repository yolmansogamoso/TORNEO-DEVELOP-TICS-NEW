@page
@model Torneo.App.Frontend.Pages.Equipos.DetailsModel
@{
}

<h1 class="titulo_tabla">Detalle del Equipo</h1>
<div class="container_tabla_details">
    <table class="tabla">
        <tr class="tr_thead">
            <th scope="col">Id</th>
            <td>@Model.equipo.Id</td>
        </tr>
        <tr>
            <th class="tabla_details_th" scope="col">Nombre</th>
            <td class="tabla_details_td">@Model.equipo.Nombre</td>
            
        </tr>
        <tr>
             <th class="tabla_details_th" scope="col">Municipio</th>
            <td class="tabla_details_td">@Model.equipo.Municipio.Nombre</td>
        </tr>
        <tr>
            <th class="tabla_details_th" scope="col">DT</th>
            <td class="tabla_details_td">@Model.equipo.DirectorTecnico.Nombre</td>
        </tr>
    </table>
    <a class="btn_volver" asp-page="./Index"><i class="fa-solid fa-chevron-left"></i></a> 
</div>


