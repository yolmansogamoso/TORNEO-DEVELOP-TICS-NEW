@page
@model Torneo.App.Frontend.Pages.DT.EditModel
@{
}
<div class="position_form">
    <h1 class="titulo_form">Editar DT</h1>
    <div class="container_form">
        <form class="flex_outer" method="post">
            <div class="form_group">
                <label asp-for="dt.Id"></label>
                <input class="form_control_edit" asp-for="dt.Id"  readonly>
            </div>
            <div class="form-group">
                <label asp-for="dt.Nombre"></label>
                <input class="form_control_edit" asp-for="dt.Nombre" >
            </div>
              <div class="form-group">
                <label asp-for="dt.Documento"></label>
                <input class="form_control_edit" asp-for="dt.Documento" >
            </div>
              <div class="form-group">
                <label asp-for="dt.Telefono"></label>
                <input class="form_control_edit" asp-for="dt.Telefono" >
            </div>
            <button type="submit" class="btn_form_edit">Editar DT</button>
        </form>
        <a class="btn-volver" asp-page="./Index">Volver</a>
    </div>
</div>